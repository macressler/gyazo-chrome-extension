{
  "name": "Gyazo",
  "description": "Grab any image on the web and share it instantly.",
  "short_name": "Gyazo",
  "version": "1.2.8",
  "author": "Nota inc.",
  "applications": {
    "gecko": {
      "id": "gyazo-extension@gyazo.com"
    }
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "notifications",
    "tabs",
    "clipboardWrite",
    "clipboardRead",
    "storage"
  ],
  "background": {
    "scripts": ["main.js", "libs/jquery-1.10.2.min.js", "libs/utils.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
        "19": "/icons/gyazo-19.png",
        "38": "/icons/gyazo-96.png"
      }
  },
  "web_accessible_resources": [
    "icons/logo.png",
    "icons/camera.png",
    "icons/close.png",
    "icons/loading.gif"
  ],
  "icons": {
    "16": "/icons/gyazo-16.png",
    "32": "/icons/gyazo-32.png",
    "48": "/icons/gyazo-48.png",
    "96": "/icons/gyazo-96.png",
    "128": "/icons/gyazo-128.png",
    "256": "/icons/gyazo-256.png"
  },
  "options_ui": {
    "page": "option/options.html",
    "chrome_style": false
  },
  "manifest_version": 2,
  "default_locale": "en"
}
